                   <ToolTipStatusbar><![CDATA[code to repeat]]></ToolTipStatusbar>
                   <EncloseSelection>true</EncloseSelection>
                </Literal>
            </Declarations>


            <Code>
                <Script Language='PowerShell' CaretOffset='0'>
                    <![CDATA[for ($x = 1000; $x -lt 15000; $x += 300) 
{
  "Frequency $x Hz"
  [Console]::Beep($x, 500)
}
]]>
                </Script>
            </Code>

